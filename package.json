{
    "name": "goto-package",
    "displayName": "Goto Package",
    "description": "Go to npm/composer package folder quickly",
    "publisher": "ctf0",
    "repository": "https://github.com/ctf0/vscode-goto-package",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.41.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "npm",
        "composer",
        "navigate"
    ],
    "activationEvents": [
        "onLanguage:json"
    ],
    "main": "./out/extension.js",
    "contributes": {},
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "yarn run compile",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.11.7",
        "@types/vscode": "^1.41.0",
        "mocha": "^6.2.2",
        "tslint": "^5.20.0",
        "typescript": "^3.6.4",
        "vscode-test": "^1.2.2"
    },
    "dependencies": {
        "lodash.debounce": "^4.0.8"
    }
}
